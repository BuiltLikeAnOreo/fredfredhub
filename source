--NORP
local Norp = Window:NewTab("Norp")
local NorpSection = Norp:NewSection("Norp")
 
NorpSection:NewButton("Norp Obelisk", "Talk to Obelisk, Don't press bye and click button.", function()
    local args = {
        [1] = {
            ["choice"] = "Resonate"
        }
    }
 
    game:GetService("ReplicatedStorage").Requests.Dialogue:FireServer(unpack(args))
end)
 
NorpSection:NewLabel("Make sure you are in the dialogue, DO NOT EXIT DIALOGUE")
 
--Player
local Player = Window:NewTab("Player")
local PlayerSection = Player:NewSection("Player")
 
PlayerSection:NewButton("Reset (READ NOTE)", "Will kill you when in danger", function()
    game.Players.LocalPlayer.Character:BreakJoints()
end)
 
PlayerSection:NewButton("Auto-regen bard meter", "Your bard meter will auto go up", function()
    while wait(5) do
        game.ReplicatedStorage.Requests.BardStreak:FireServer(40)
    end
end)
PlayerSection:NewLabel("Spam auto-regen meter for better results")
--Extra Scripts
local Scripts = Window:NewTab("Extra Scripts")
local ScriptsSection = Scripts:NewSection("Scripts")
 
ScriptsSection:NewButton("Infinite Yield", "Basic Roblox 'admin commands' script", function()
    loadstring(game:HttpGet('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'))()
end)
 
ScriptsSection:NewLabel("TIP use clicktp and gate same time to get to norp ob")
