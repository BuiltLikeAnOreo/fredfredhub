local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("fredrick hub V.3.0.6", "Ocean")
 
-- main
local main = Window:NewTab("main")
local mainSection = main:NewSection("Main")
 
 
mainSection:NewToggle("Auto Trinket Pickup (risky)", "Pick up trinket then go back for it to work", function(state)
        if state then
            loadstring(game:HttpGet("https://pastebin.com/raw/E2i4yJrZ", true))()
        else
            loadstring(game:HttpGet("https://pastebin.com/raw/E2i4yJrZ", false))()
        end
end)
 
 
mainSection:NewToggle("fly (Read Note)", "(C 2Fly Cant turn off)", function(state)
    if state then
        loadstring(game:HttpGet("https://pastebin.com/raw/iU1amDnf", true))()
    else
        loadstring(game:HttpGet("https://pastebin.com/raw/iU1amDnf", false))()
    end
end)
 
 
mainSection:NewToggle("nofall (Read Note)", "(CAUTION Once on cant turn off)", function(state)
    if state then
        loadstring(game:HttpGet("https://pastebin.com/raw/rGFWSeNd", true))()
    else
        loadstring(game:HttpGet("https://pastebin.com/raw/rGFWSeNd", false))()
    end
end)
 
--ESP
local ESP = Window:NewTab("ESP")
local ESPSection = ESP:NewSection("ESP")
 
ESPSection:NewToggle("just unNamed", "YEah", function(state)
    if state then
        loadstring(game:HttpGet('https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua', true))()
    else
        loadstring(game:HttpGet('https://raw.githubusercontent.com/ic3w0lf22/Unnamed-ESP/master/UnnamedESP.lua', false))()
    end
end)
 
ESPSection:NewButton("LeaderboardESP", "RightClick on their name to view.", function()
    loadstring(game:HttpGet('https://pastebin.com/raw/21SA0uTb', true))()
end)
 
--RACE
local Race = Window:NewTab("Race")
local RaceSection = Race:NewSection("Races")
 
RaceSection:NewButton("Dzin", "Talk to NPC, Don't press bye and click button.", function()
    local args = {
        [1] = {
            ["choice"] = "Dzin"
        }
    }
 
    game:GetService("ReplicatedStorage").Requests.Dialogue:FireServer(unpack(args))
end)
 
